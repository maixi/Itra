@model List<Demotivator>
@{
    ViewBag.Title = "Home Page";
}
<a href="#x" class="overlay" id="win5"></a>
<link rel="stylesheet" href="~/Content/lightbox.css" />
<link rel="stylesheet" href="~/Content/owl.carousel.css" />
<link rel="stylesheet" href="~/Content/owl.theme.css" />
<link rel="stylesheet" href="~/Content/owl.transitions.css" />
<script src="~/Scripts/jquery-1.9.1.min.js"></script>
<script src="~/Scripts/owl.carousel.js"></script>
<script src="~/Scripts/lightbox.js"></script>
<head>
    <meta charset="utf-8">
    <title>nth-child</title>
    <style>
        table {
            width: 100%; /* Ширина таблицы */
            border-spacing: 10px; /* Расстояние между ячейками */
            border-color: #000000;
            float: left;
            background-color: #ff6a00;
        }

        #owl-demo .item {
            margin: 3px;
            height:inherit;
            align-items:center;
        }

            #owl-demo .item img {
                width: 100%;
                height:auto;
                
            }
    </style>
</head>
<h1>Новое</h1>
<hr />
<dl class="dl-horizontal">
    
    <div class="owl-carousel" id="owl-demo">
        @foreach (var item in Model)
        {
            <div class="item">
                <a class="example-image-link" href=@item.DemotivatorUrl data-lightbox="example-1">
                <img class="example-image"  src=@item.DemotivatorUrl alt="image-1" />
                </a>
                <div class="td" id="ShowRe">@Html.Partial("_VoteShow", item.Rate)</div>
            </div>
        }
    </div>


</dl>
<h1>Лучшее</h1>
<hr />
<footer>
    <div id="myCanvasContainer" style="width:100%;float:left;">

        <canvas width="1300" height="300" id="myCanvas">
            <!--       <p>Anything in here will be replaced on browsers that support the canvas element</p>-->

            <li><a href="/fish">SJDHLKASJHDKLSA</a></li>
            <li><a href="/chips">Chips</a></li>
            <li><a href="/salt">Salt</a></li>
            <li><a href="/vinegar">Vinegar</a></li>

        </canvas>
    </div>
</footer>
<script>
    $(document).ready(function () {

        $("#owl-demo").owlCarousel({

            autoPlay: 3000, //Set AutoPlay to 3 seconds

            items: 4,
            itemsDesktop: [1199, 3],
            itemsDesktopSmall: [979, 3],
            pagination: true
            

        });

    });
</script>