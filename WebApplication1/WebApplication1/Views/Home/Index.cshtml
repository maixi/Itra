@model List<Demotivator>
@{
    ViewBag.Title = "Home Page";
}
@if (!User.Identity.IsAuthenticated)
{
    <div>Log in</div>
}
<a href="#x" class="overlay" id="win5"></a>
<div class="popup">
    <img class="is-image" src="" alt="" height="100" />
    <a class="close" title="Закрыть" href="#close"></a>
    
</div>
<link rel="stylesheet" href="~/Content/lightbox.css">
<script src="~/Scripts/jquery-1.10.2.min.js"></script>
<script src="~/Scripts/lightbox.js"></script>
<head>
    <meta charset="utf-8">
    <title>nth-child</title>
    <style>
        table {
            width: 100%; /* Ширина таблицы */
            border-spacing: 10px; /* Расстояние между ячейками */
            border-color: #000000;
            float: left;
            background-color: #ff6a00;
        }
    </style>
</head>
<dl class="dl-horizontal">
    @foreach (var item in Model)
    {

        <a class="example-image-link" href=@item.OriginalImageUrl data-lightbox="example-1"><img class="example-image" src=@item.OriginalImageUrl alt="image-1" width="100" height="111" /></a>

    }

</dl>
<footer>
    <div id="myCanvasContainer" style="width:100%;float:left;">

        <canvas width="1300" height="300" id="myCanvas">
            <!--       <p>Anything in here will be replaced on browsers that support the canvas element</p>-->

            <li><a href="/fish">SJDHLKASJHDKLSA</a></li>
            <li><a href="/chips">Chips</a></li>
